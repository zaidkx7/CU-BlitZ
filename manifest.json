{
  "manifest_version": 3,
  "author": "Muhammad Zaid",
  "name": "CU-BlitZ",
  "description": "Blitz Through CUSIT Evaluations. Unlock Your Results, Because You have better things to do than rating professors.",
  "version": "1.0.1",
  "permissions": ["storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/image-16x16.jpg",
      "48": "icons/image-48x48.jpg",
      "128": "icons/image-128x128.jpg"
    },
    "default_title": "CU-BlitZ"
  },
  "icons": {
    "16": "icons/image-16x16.jpg",
    "48": "icons/image-48x48.jpg",
    "128": "icons/image-128x128.jpg"
  },
  "content_scripts": [
    {
      "matches": ["https://*.cu.edu.pk/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["https://*.cu.edu.pk/*"]
}

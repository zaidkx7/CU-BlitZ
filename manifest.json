{
  "manifest_version": 3,
  "author": "Muhammad Zaid",
  "name": "UniBlitz",
  "description": "Blitz Through Evaluations. Unlock Your Results, Because Youâ€™ve Got Better Things Than Rating Professors.",
  "version": "1.0",
  "permissions": ["storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/image-16x16.jpg",
      "48": "icons/image-48x48.jpg",
      "128": "icons/image-128x128.jpg"
    },
    "default_title": "UniBlitz"
  },
  "icons": {
    "16": "icons/image-16x16.jpg",
    "48": "icons/image-48x48.jpg",
    "128": "icons/image-128x128.jpg"
  },
  "content_scripts": [
    {
      "matches": ["https://www.cu.edu.pk/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["https://www.cu.edu.pk/*"]
}
